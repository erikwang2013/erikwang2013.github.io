        &amp;lt;property&amp;gt;
            &amp;lt;name&amp;gt;dfs.namenode.secondary.http-address&amp;lt;/name&amp;gt;
            &amp;lt;value&amp;gt;ec1069fcab62:9001&amp;lt;/value&amp;gt;
        &amp;lt;/property&amp;gt;
        &amp;lt;property&amp;gt;
        &amp;lt;name&amp;gt;dfs.webhdfs.enabled&amp;lt;/name&amp;gt;
        &amp;lt;value&amp;gt;true&amp;lt;/value&amp;gt;
        &amp;lt;/property&amp;gt;
    &amp;lt;/configuration&amp;gt;
&lt;/code&gt;&lt;/pre&gt;&lt;p&gt;&lt;br /&gt; &lt;/p&gt;
&lt;ul&gt;
&lt;li&gt;&lt;h5 id=&#34;mapred-site-xml配置&#34;&gt;&lt;a href=&#34;#mapred-site-xml配置&#34; class=&#34;headerlink&#34; title=&#34;mapred-site.xml配置&#34;&gt;&lt;/a&gt;mapred-site.xml配置&lt;/h5&gt;&lt;/li&gt;
&lt;/ul&gt;
